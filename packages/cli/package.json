{
  "name": "@sheikah-converter/cli",
  "version": "1.0.0-alpha.0",
  "description": "CLI to convert BotW save files between console formats",
  "author": "Ian Mancini",
  "bin": {
    "botwc": "./bin/run"
  },
  "homepage": "https://github.com/ametis70/sheikah-converter/tree/main/packages/cli#readme",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "repository": "ametis70/sheikah-converter",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@sheikah-converter/lib": "^1.0.0-alpha.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^16.18.34",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "stdout-stderr": "^0.1.13",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "botwc",
    "commands": "./dist",
    "default": ".",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/ametis70/sheikah-converter/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
