{
  "name": "@sheikah-converter/cli",
  "version": "0.0.0",
  "description": "CLI to convert BotW save files between console formats",
  "author": "Ian Mancini @ametis70",
  "bin": {
    "sheikahcli": "./bin/run"
  },
  "homepage": "https://github.com/ametis70/sheikah-converter",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "ametis70/sheikah-converter",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@sheikah-converter/lib": "*"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.8.0",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "sheikahcli",
    "dirname": "sheikahcli",
    "commands": "./dist",
    "default": ".",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/ametis70/sheikah-converter/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
